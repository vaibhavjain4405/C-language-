#include<stdio.h>

int main(){
    int l,b,Area, Perimeter;
    
    printf("Enter the length of the rectangle = ");
    scanf("%d",&l);
    
    printf("Enter the breath of the rectangle = ");
    scanf("%d",&b);
    
    Area = l*b;
    Perimeter = 2*(l+b);
    
    printf("Area of the rectangle is = %d\n",Area);
    printf("Perimeter of the rectangle is = %d",Perimeter);
    return 0;
}
